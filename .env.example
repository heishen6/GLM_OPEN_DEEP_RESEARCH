# 智谱AI GLM-4.5 配置
OPENAI_API_KEY=xxxx
OPENAI_BASE_URL=https://open.bigmodel.cn/api/paas/v4/

# 其他模型的API Keys（如果需要）
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# 搜索API配置 (必填)
TAVILY_API_KEY=

# LangSmith追踪配置（可选）
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=
LANGSMITH_TRACING=false

# 模型配置（使用智谱AI GLM-4.5,glm-4.5-air）
SUMMARIZATION_MODEL=openai:glm-4.5-air
RESEARCH_MODEL=openai:glm-4.5-air
COMPRESSION_MODEL=openai:glm-4.5-air
FINAL_REPORT_MODEL=openai:glm-4.5

# Only necessary for Open Agent Platform
SUPABASE_KEY=
SUPABASE_URL=
# Should be set to true for a production deployment on Open Agent Platform. Should be set to false otherwise, such as for local development.
GET_API_KEYS_FROM_CONFIG=false